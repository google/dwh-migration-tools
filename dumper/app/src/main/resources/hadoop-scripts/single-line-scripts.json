{
  "ip-address": "ip -o -4 a show",
  "ls-usr-local-lib": "ls -l /usr/local/lib",
  "ls-var-log": "ls -l /var/log",
  "os-release": "cat /etc/os-release",
  "hive-concurrency-beeline": "beeline -u jdbc:hive2:// -e 'set hive.support.concurrency'",
  "hive-txn-manager-beeline": "beeline -u jdbc:hive2:// -e 'set hive.txn.manager'",
  "hive-execution-engine-beeline": "beeline -u jdbc:hive2:// -e 'set hive.execution.engine'",
  "hive-concurrency": "hive -e 'set hive.support.concurrency'",
  "hive-txn-manager": "hive -e 'set hive.txn.manager'",
  "hive-execution-engine": "hive -e 'set hive.execution.engine'",
  "timedatectl-status": "timedatectl status",
  "hadoop-checknative": "hadoop checknative -a",
  "dmidecode-processor": "sudo -n dmidecode --type processor",
  "blkid": "sudo -n blkid",
  "hdfs-crypto-zones": "hdfs crypto -listZones",
  "hdfs-replication": "hdfs getconf -confKey dfs.replication",
  "hadoop-security-authorization": "hdfs getconf -confKey hadoop.security.authorization",
  "hadoop-security-authentication": "hdfs getconf -confKey hadoop.security.authentication",
  "hadoop-rpc-protection": "hdfs getconf -confKey hadoop.rpc.protection",
  "hadoop-ssl-enabled": "hdfs getconf -confKey hadoop.ssl.enabled",
  "hadoop-ssl-require-client-cert": "hdfs getconf -confKey hadoop.ssl.require.client.cert",
  "hadoop-ssl-server": "hdfs getconf -confKey hadoop.ssl.server",
  "hadoop-ssl-client": "hdfs getconf -confKey hadoop.ssl.client"
}
